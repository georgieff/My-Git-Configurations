[user]
  name = georgieff
  email = lubo@georgieff.it
[color "branch"]
  current = red reverse white
  local = white
  remote = green
[color]
  ui=auto
[color "diff"]
  meta = BLACK bold
  old = red bold
  new = green bold
  whitespace = red reverse white bold
[color "status"]
  added = green
  untracked = yellow
[core]
  editor = 'C:/Program Files/Sublime Text 2/sublime_text.exe' -w
[push]
  default = simple
[init]
  templatedir = ~/GitConfig/templates/
[alias]
  s = status
  c = commit
  co = checkout
  br = branch